Begin
	get(x);
	put("Value of x= "); put(x);
	z := x * w234;
	ch := str(3);
	if (z >= 5 ) then
	  	z := z + Y_1;
	  	x := x + 1;
	elsif ( z > 2) then
		z := z - Y_1;
	  	x := x - 1;
